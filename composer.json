{
  "name": "miaoxing/cart",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Cart Plugin",
  "keywords": [
    "miaoxing",
    "cart"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.3",
    "miaoxing/product": "^0.1.19",
    "miaoxing/user": "^0.2.17"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Cart\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Cart\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "wei/wei": "twinh/wei as 0.10.x-dev",
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev",
      "miaoxing/product": "miaoxing/dev as 0.1.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  }
}
