{
  "name": "miaoxing/cart",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Cart Plugin",
  "keywords": [
    "miaoxing",
    "cart"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.4",
    "miaoxing/product": "^0.4.3",
    "miaoxing/user": "^0.4.7"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Cart\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Cart\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "wei/wei": "twinh/wei as 0.12.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.8.x-dev",
      "miaoxing/product": "miaoxing/product as 0.4.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.1"
  }
}
