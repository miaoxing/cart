{
  "name": "miaoxing/cart",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Cart Plugin",
  "keywords": [
    "miaoxing",
    "cart"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.7",
    "miaoxing/product": "^0.1.4",
    "miaoxing/user": "^0.2.2"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Cart\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Cart\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
