{
  "name": "miaoxing/cart",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Cart Plugin",
  "keywords": [
    "miaoxing",
    "cart"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.7",
    "miaoxing/product": "^0.4.6",
    "miaoxing/user": "^0.4.10"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Cart\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Cart\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "wei/wei": "twinh/wei as 0.13.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.9.x-dev",
      "miaoxing/product": "miaoxing/product as 0.4.x-dev",
      "miaoxing/services": "miaoxing/services as 0.6.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.3"
  }
}
